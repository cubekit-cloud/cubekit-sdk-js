{
  "name": "@cubekit-cloud/cubekit-sdk-js",
  "version": "1.0.17",
  "description": "@cubekit-cloud/cubekit-sdk-js - the library for working with auto generated API from cubekit.com",
  "types": "dist/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "prepublish": "npm run build",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "docs": "jsdoc2md -c ./jsdoc2md.json -t ./README.hbs ./src/core/CubekitOrmClient.ts > ./README.md",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cubekit-cloud/cubekit-sdk-js.git"
  },
  "keywords": [
    "cubekit",
    "sdk",
    "cubekit-client",
    "cubekit-cloud"
  ],
  "author": "Ivan Fontosh",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-loader": "^9.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "eslit": "^6.0.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^8.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "rollup": "^3.14.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup-plugin-dts": "^5.1.1"
  },
  "lint-staged": {
    "*.*": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^1.6.7"
  }
}
